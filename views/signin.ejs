  <body class="text-center">
    <form class="form-signin" action="/signin" method="post" name="Login_Form" >
  <img class="mb-4" src="/img/logo_sm_300.png">

	<% if (message) { %>
	<p><%= message %></p>
	<% } %>
  <label for="inputEmail" class="sr-only">Email address</label>

  <input type="email" name="username" class="form-control" placeholder="Email address"  autocomplete="new-email" required autofocus>
  <label for="inputPassword" class="sr-only">Password</label>
  <input type="password" name="password" class="form-control" placeholder="Password" autocomplete="new-password" required>
  <div class="checkbox mb-3">
    <label>
      <input type="checkbox" value="remember-me"> Remember me
    </label>
  </div>
  <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>

</form>
