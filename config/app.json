            "esversion" : 6,
            "maxerr" :250
          }, 
        "debug" : "ejs",
        "manifest" : true
    },
    "server" : {
        "routes" : {
            "/" : "index",
            "/verify" : "verify",
            "/status" : "status",
            "/api/accounts" : "accounts"
